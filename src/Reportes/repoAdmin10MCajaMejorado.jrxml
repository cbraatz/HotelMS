	<queryString><![CDATA[SELECT distinct(m.idmov),
     ape.feyhoape, ape.feyhocie,
     u.usuario,
     m.feyhomov, m.montomov, m.montogscondesc as montogs, t.descripconc as tipomovimiento, mon.moneda, f.descripformapa as formapago
FROM
     audiapeyciecaja as ap INNER JOIN apeyciecaja as ape on ap.idayccaja = ape.idayccaja INNER JOIN movcaja as m on m.idayccaja = ape.idayccaja INNER JOIN audimovcaja as am on m.idmov = am.idmov
     INNER JOIN usuarios as u on u.idusuario = am.idusuario INNER JOIN tipomov as t on t.idtipomov = m.idtipomov INNER JOIN moneda as mon on mon.idmoneda = m.idmoneda INNER JOIN formapago as f on m.idforma = f.idforma 
WHERE ap.movimiento = 'APERTURA' and m.idtipomov <> 3 and m.feyhomov >= $P{fecha1} and m.feyhomov <= $P{fecha2}order by ape.feyhoape, m.idmov;]]></queryString>

